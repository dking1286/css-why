{
  "scripts": {
    "build": "dune build @frontend",
    "watch": "dune build @frontend -w",
    "bundle": "webpack --mode production --output-path ./frontend/dist --entry ./_build/default/frontend/bin/output/frontend/bin/Main.js",
    "serve": "webpack serve --open --mode development --static frontend/public --entry ./_build/default/frontend/bin/output/frontend/bin/Main.js"
  },
  "devDependencies": {
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
}